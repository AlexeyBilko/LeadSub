<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - LeadSub</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/LeadSub.styles.css" asp-append-version="true" />
    <link href="~/css/mainpage.css" rel="stylesheet" />
     <link href="~/css/account.css" rel="stylesheet" />
    <link href="~/css/site.css" rel="stylesheet" />
</head>
<body>

 <div class="container">
   
<nav class="navbar navbar-expand-lg">
    <div class="container-fluid col-xxl-11">
     <a href="/" class="main-logo ms-auto me-auto navber-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">Lead<span style="color: #3362DB">Sub</span></a>
    <div class="collapse navbar-collapse" id="navbarSupportedContent" style="height:80px">
      <ul class="navbar-nav me-auto mb-2 mb-lg-0">
        <li class="nav-item" >
          <a class="nav-link main-menu-item" style="margin-left:150px" aria-current="page" href="#">MySubPages</a>
        </li>
        <li class="nav-item ms-5 me-5">
          <a class="nav-link main-menu-item" href="#">Billing</a>
        </li>
         <li class="nav-item ms-5 me-5">
          <a class="nav-link main-menu-item" href="#">Links Page</a>
        </li>
         <li class="nav-item ms-5">
          <div class="top-main-button w-100">
              <a href="#" class="main-button-text">Create page</a>
          </div>
        </li>
      </ul>
  </div>
  </div>
</nav>
    
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
                <footer class="main-footer">
    <div class="footer-logo">
        <a href="/" class="footer-main-logo">Lead<span style="color: #3362DB">Sub</span></a>
    </div>
    <div class="col-xxl-4 make-center">
            <a href="/" class="social-media-link d-flex justify-content-md-start">
                <div class="social-media-image">
                    <img src="/images/instagram-logo.png"/>
                </div>
                <p class="ms-2 social-media-title">Instagram</p>
            </a>

            <a href="/" class="social-media-link d-flex justify-content-md-start">
                <div class="social-media-image">
                    <img src="~/images/youtube-logo.png"/>
                </div>
                <p class="ms-2 social-media-title">YouTube</p>
            </a>
        
            <a href="/" class="social-media-link d-flex justify-content-md-start">
                <div class="social-media-image">
                    <img src="~/images/Twitter-logo.svg.png"/>
                </div>
                <p class="ms-2 social-media-title">Twitter</p>
            </a>
    </div>

        <div class="footer-links">
            <div>
                <a href="/" class="footer-link">FAQ</a>
            </div>
            <div>
                <a href="/" class="footer-link">Pricing</a>
            </div>
            <div>
                <a href="/" class="footer-link">Contact Us</a>
            </div>
        </div>
</footer>

    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)


</body>
</html>
